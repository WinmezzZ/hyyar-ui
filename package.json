{
  "name": "hyyar-ui",
  "version": "0.0.3",
  "description": "A Morden React UI Library.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "esm/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "authors": {
    "name": "Winme",
    "email": "winme0308@gmail.com"
  },
  "keywords": [
    "hyyar-ui",
    "react",
    "react-library"
  ],
  "scripts": {
    "dev": "docz dev",
    "start": "npm run dev",
    "clean": "rimraf lib esm dist",
    "dev:doc": "docz serve",
    "build:doc": "rimraf doc-site && docz build",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "npm run clean && gulp && npm run build:types ",
    "release": "node ./scripts/release.js",
    "new": "plop --plopfile ./scripts/plopfile.js",
    "lint": "tsc --pretty && eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WinmezzZ/hyyar-ui.git"
  },
  "bugs": {
    "url": "https://github.com/WinmezzZ/hyyar-ui/issues"
  },
  "homepage": "https://github.com/WinmezzZ/hyyar-ui.git",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.14",
    "@types/prismjs": "^1.16.1",
    "@types/react": "^16.9.44",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "@types/testing-library__react": "^10.2.0",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.1",
    "conventional-changelog-cli": "^2.1.0",
    "cpr": "^3.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "doctoc": "^1.4.0",
    "docz": "^2.3.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby-plugin-less": "^4.0.6",
    "gatsby-plugin-styled-components": "^3.3.13",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.2",
    "less": "^3.12.2",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "plop": "^2.7.4",
    "prettier": "^2.0.5",
    "prismjs": "^1.21.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-simple-code-editor": "^0.11.0",
    "react-tooltip": "^4.2.10",
    "react-use": "^15.3.4",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "styled-components": "^5.2.0",
    "through2": "^4.0.2",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "npm run lint",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
